    "url": "https://github.com/jgrey4296/exclusionLogic/issues"
  },
  "homepage": "https://github.com/jgrey4296/exclusionLogic#readme",
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.2.1",
    "babel-register": "^6.24.0",
    "chai": "^3.5.0",
    "eslint": "^3.17.1",
    "eslint-loader": "^1.6.3",
    "flow-babel-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.41.0",
